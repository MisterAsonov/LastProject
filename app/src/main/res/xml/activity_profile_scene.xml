<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000"
        motion:motionInterpolator="easeInOut"
        >
        <OnSwipe
            motion:dragDirection="dragUp"
            motion:touchAnchorId="@id/layout"
            motion:touchRegionId="@id/layout"
            motion:touchAnchorSide="top"
            motion:onTouchUp="autoComplete"
            />


       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint android:id="@+id/photo">

            <CustomAttribute
                motion:attributeName="roundPercent"
                motion:customFloatValue="0.0000001"/>



        </Constraint>
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end"
        >

        <Constraint
            android:id="@+id/photo"
            android:layout_height="?attr/actionBarSize"
            android:layout_width="?attr/actionBarSize"
            android:layout_margin="5dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent">
            <CustomAttribute
            motion:attributeName="roundPercent"
            motion:customFloatValue="1"/>
        </Constraint>

        <Constraint
            android:id="@+id/textView5"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toTopOf="@id/textView6"
            motion:layout_constraintStart_toEndOf="@id/photo"
            motion:layout_constraintHorizontal_bias="0.0"
            android:layout_marginStart="8dp"
            android:layout_marginTop="5dp" />
        <Constraint
            android:id="@+id/textView6"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@id/photo"
            android:layout_marginBottom="8dp"
            motion:layout_constraintStart_toEndOf="@id/photo"
            motion:layout_constraintHorizontal_bias="0.0"
            android:layout_marginStart="8dp" />
    </ConstraintSet>
</MotionScene>